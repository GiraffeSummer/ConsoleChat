{
  "name": "consolechat",
  "version": "1.0.0",
  "description": "ConsoleChat app",
  "main": "index.js",
  "dependencies": {
    "boxen": "^5.0.1",
    "chalk": "^4.1.1",
    "figlet": "^1.5.0",
    "readline-sync": "^1.4.10",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2"
  },
  "scripts": {
    "start": "node index.js",
    "build": "pkg . --target host --output build/ConsoleChat.exe --debug",
    "server3000": "node index.js mode=2 port=3000",
    "client": "node index.js mode=1 uri=http://google.com"
  },
  "pkg": {
    "assets": "node_modules/**/*",
    "output": "build",
    "outputPath": "build"
  },
  "bin": "index.js",
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "pkg": "^5.3.0"
  },
  "keywords": [
    "console",
    "chat"
  ],
  "author": "Me",
