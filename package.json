{
  "name": "consolechat",
  "version": "0.1.3",
  "description": "ConsoleChat app",
  "main": "index.js",
  "dependencies": {
    "boxen": "^6.2.1",
    "chalk": "^5.0.1",
    "express": "^4.18.1",
    "figlet": "^1.5.2",
    "readline-sync": "^1.4.10",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0"
  },
  "scripts": {
    "start": "node index.js",
    "build": "pkg . --target host --output build/ConsoleChat.exe --debug",
    "server3000": "node index.js mode=2 port=3000",
    "client": "node index.js mode=1 uri=http://google.com"
  },
  "pkg": {
    "assets": "node_modules/**/*",
    "outputPath": "build",
    "scripts": "commands/*.js"
  },
  "bin": "index.js",
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "console",
    "chat"
  ],
  "author": "Me",
  "license": "ISC",
  "devDependencies": {
    "pkg": "^5.6.0"
  }
}
